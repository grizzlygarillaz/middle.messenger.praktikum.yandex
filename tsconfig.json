{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "rootDir": "./src",
    "baseUrl": "./src",
    "outDir": "./dist",
  }
}
